{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
    "types": ["vite/client"],
    "paths": {
      "@data": ["src/data"],
      "@data/*": ["src/data/*"],
      "@images": ["src/images"],
      "@images/*": ["src/images/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@lib": ["src/lib"],
      "@lib/*": ["src/lib/*"],
      "@utils": ["src/lib/utils"],
      "@utils/*": ["src/lib/utils/*"],
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"]
    }
  }
}
